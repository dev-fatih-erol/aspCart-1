<!-- Best Sells Area Start -->
<section class="best-sells-area mb-30px">
    <div class="container">
        <!-- Section Title Start -->
        <div class="row">
            <div class="col-md-12">
                <div class="section-title">
                    <h2>Best Sellers</h2>
                    <p>Add bestselling products to weekly line up</p>
                </div>
            </div>
        </div>
        <!-- Section Title End -->
        <!-- Best Sell Slider Carousel Start -->
        @if (Model.Count() > 0)
        {
            <div class="best-sell-slider owl-carousel owl-nav-style">
                @foreach (var product in Model)
                {
                    <!-- Single Item -->
                    <article class="list-product">
                        <div class="img-block">
                            <a href="/Product/@product.SeoUrl" class="thumbnail">
                                @if (product.MainImage != null)
                                {
                                    <img class="first-img" src="@product.MainImage?w=170&h=170" alt="@product.Name" />
                                }
                                else
                                {
                                    <img class="first-img" src="~/images/system/no-image.png" alt="@product.Name" />
                                }
                                <!--<img class="second-img" src="https://localhost:62124/images/test_images/rog%20strix%20rx480%20O8G%20gaming.jpg?w=170&h=170" alt="" />-->
                            </a>
                        </div>
                        <ul class="product-flag">
                            @if (product.OldPrice > 0)
                            {
                                <li class="new">Yeni</li>
                            }
                        </ul>
                        <div class="product-decs">
                            <h2><a href="/Product/@product.SeoUrl" class="product-link">@product.Name</a></h2>
                            <div class="rating-product">
                                @for (int i = 0; i < Math.Ceiling(product.Rating); i++)
                                {
                                    <i class="ion-android-star"></i>
                                }
                                @for (int i = 0; i < 5 - Math.Ceiling(product.Rating); i++)
                                {
                                    <i class="ion-android-star-outline"></i>
                                }
                            </div>
                            <div class="pricing-meta">
                                <ul>
                                    @if (product.OldPrice > 0)
                                    {
                                        <li class="old-price">@product.OldPrice.ToString("N2") TL</li>
                                        <li class="current-price">@Html.Raw(product.Price.ToString("N2")) TL</li>
                                        <li class="discount-price">%@Math.Ceiling((product.OldPrice - product.Price) / product.Price * 100)</li>
                                    }
                                    else
                                    {
                                        <li class="current-price">@product.Price.ToString("N2") TL</li>
                                    }
                                </ul>
                            </div>
                        </div>
                        <div class="add-to-link">
                            <ul>
                                <li class="cart">
                                    <form asp-area="" asp-controller="Cart" asp-action="Add" method="post">
                                        <input type="hidden" value="@product.Id" name="id" />
                                        <button type="submit" class="cart-btn">ADD TO CART</button>
                                    </form>
                                </li>
                            </ul>
                        </div>
                    </article>
                }
            </div>
            <!-- Best Sells Carousel End -->
        }
    </div>
</section>
<!-- Best Sells Slider End -->
